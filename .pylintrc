[MASTER]
ignore=venv
extension-pkg-whitelist=

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    line-too-long,
    trailing-whitespace

[FORMAT]
max-line-length=88
function-rgx=[a-z_][a-z0-9_]{2,30}$
variable-rgx=[a-z_][a-z0-9_]{2,30}$
class-rgx=[A-Z][a-zA-Z0-9]+$
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__))$
